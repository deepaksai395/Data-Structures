#include <stdio.h>
#include <stdlib.h>
#define MAX 20
void enqueue(int arr[MAX]);
void dequeue(int arr[MAX]);
void display(int arr[MAX]);
int arr[MAX]; int f=-1, r=-1; int value,i;
void main()
{
    int  input; int arr[MAX];
    printf("enter the choice\n");
    printf("1.enqueue\n2.dequeue\n3.exit");
scanf("%d",&input);
    while(input!=3){

    switch(input)
    {
       case 1: enqueue(arr); display(arr);  break;
       case 2: dequeue(arr); display(arr); break;
    }
 printf("enter the choice\n");
         scanf("%d",&input);
    }
}

void enqueue(int arr[MAX])
{   printf("enter the value");
    scanf("%d",&value);
    if(f==0 && r==MAX-1){printf("no room for more elements");}
    if(f==-1 && r==-1)
    {
       f=r=0;
    }
    else if(r==MAX-1 && f!=0)
    {
        r=0;
    }
    else{r=r+1;}
    arr[r]=value;
}

void dequeue(int arr[MAX])
{
    if(f==-1){printf("UNDERFLOW");}
    if(f==r){f=r=-1;}
    else if(f==MAX-1){f=0;}
    else{f+=1;}
}
void display(int arr[MAX])
{ printf("the queue is\n");
    for(i=f;i<=r;i++)
    {
        printf("%d\t",arr[i]);
    }
}
